void print(int n){
for(int i=0;i<n;i++){
    
for(int j=0;j<n;j++){
    if(j==0 || j==n-1 || i==0 || i==n-1){
        cout<<4;
    }
    else if(j==1 || j==n-2 || i==1 || i==n-2 ){
        cout<<3;
    }
    else if(j==2 || j==n-3 || i==2 || i==n-3 ){
        cout<<2;
    }
    else{
        cout<<1;
    }
    
}
    cout<<endl;
}
}
