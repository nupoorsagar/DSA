void print(int n){
 for(int i=1;i<=n;i++){
     for(int j=1;j<=i;j++){
         cout<<"*";
     }
     for(int j=1;j<=2*(n-i);j++){
         cout<<" ";
     }
      for(int j=1;j<=i;j++){
         cout<<"*";
     }
     cout<<endl;
 }
  for(int i=1;i<n;i++){
     for(int j=1;j<=n-i;j++){
         cout<<"*";
     }
     for(int j=1;j<=2*i;j++){
         cout<<" ";
     }
      for(int j=1;j<=n-i;j++){
         cout<<"*";
     }
     cout<<endl;
 }
 
}
